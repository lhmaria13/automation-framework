{
  "browser": {
    "browser_type": "chrome",
    "headless": false,
    "implicit_wait": 10,
    "page_load_timeout": 30,
    "window_size": "1920,1080",
    "proxy": null,
    "user_data_dir": null
  },
  "logging": {
    "level": "INFO",
    "log_dir": "logs",
    "max_bytes": 10485760,
    "backup_count": 5
  },
  "desktop": {
    "timeout": 10,
    "pause_between_actions": 0.5,
    "screenshot_on_error": true
  },
  "console": {
    "timeout": 30,
    "encoding": "utf-8",
    "capture_output": true
  }
}
