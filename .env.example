# Exemplo de arquivo .env para credenciais e configurações
# Copie este arquivo para .env e preencha os valores reais

# Configurações de Navegador
AUTO_BROWSER_HEADLESS=false
AUTO_BROWSER_TYPE=chrome
AUTO_BROWSER_IMPLICIT_WAIT=10

# Configurações de Desktop
AUTO_DESKTOP_TIMEOUT=10
AUTO_DESKTOP_PAUSE_BETWEEN_ACTIONS=0.5

# Configurações de Console
AUTO_CONSOLE_TIMEOUT=30

# Credenciais (não commit este arquivo se preenchido)
APP_USERNAME=seu_usuario
APP_PASSWORD=sua_senha
APP_URL=https://sua-aplicacao.com

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=admin
DB_PASSWORD=senha

# Proxy (opcional)
PROXY_URL=
PROXY_PORT=
